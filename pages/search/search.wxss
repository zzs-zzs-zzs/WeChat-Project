page {
  background-color: #eeeeee;
  width: 100%;
  height: 100%;
}
page .search {
  background-color: white;
  width: 100%;
  display: flex;
  height: 90rpx;
  padding: 10rpx;
  box-sizing: border-box;
}
page .search .inputBox {
  flex: 9;
  display: flex;
  border: 1px solid #eeeeee;
  border-radius: 35rpx;
}
page .search .inputBox .inputView {
  flex: 12;
  display: flex;
  align-items: center;
}
page .search .inputBox .inputView .input {
  margin-left: 25rpx;
}
page .search .inputBox .icon {
  flex: 1;
  display: flex;
  align-items: center;
}
page .search .searchButton {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 33rpx;
  color: gray;
  margin-left: 10rpx;
}
page .content {
  font-size: 25rpx;
  width: 100%;
  background-color: white;
  margin-top: 25rpx;
  border-radius: 20rpx;
}
page .content .item {
  width: 100%;
  height: 35rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 25rpx;
  border-bottom: 1px solid #eeeeee;
}
